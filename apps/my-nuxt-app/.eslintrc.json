{
  "extends": [
    "@nuxt/eslint-config",
    "../../.eslintrc.json",
    "../../.eslintrc.base.json"
  ],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx", "*.vue"],
      "rules": {
        "vue/multi-word-component-names": "off"
      }
    }
  ]
}
